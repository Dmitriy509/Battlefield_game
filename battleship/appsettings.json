{
  "Logging": {

    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "File1": {
      "pathFormat": "C:/log/log1.txt",
      "fileSizeLimitBytes": 10485760,
      "retainedFileCountLimit": 10,
      "LogLevel": {
        "WebHost": "Error",
        "MyApp": "Information",
        "Default": "Error"
      }
    },
    "File2": {
      "pathFormat": "C:/log/log2.txt",
      "fileSizeLimitBytes": 10485760,
      "retainedFileCountLimit": 10,
      "LogLevel": {
        "WebHost": "information",
        "MyApp": "Critical",
        "Default": "information"
      }
    }

  },

  "AllowedHosts": "*"
}
